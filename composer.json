{
    "name": "wdalmut/frankie-doctrine-module",
    "description": "Add Doctrine ORM to your project",
    "license": "MIT",
    "require": {
        "wdalmut/frankie-modular": "dev-master",
        "doctrine/orm": "~2",
        "psr/container": "~1",
        "zendframework/zend-servicemanager": "~3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Corley\\DoctrineModule\\": ["./src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corley\\DoctrineModule\\": ["./tests"]
        }
    }
}
